<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="priv.mh.dao.IGoodsDao">
    <select id="listAllGoods" resultType="goods">
        select id as gid,
               goodname as gname,
               price,
               quantity
        from goods
    </select>
</mapper>